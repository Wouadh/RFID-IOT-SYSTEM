    {
        "id": "bd9c5513.754a68",
        "type": "function",
        "z": "f5f08b1f5f22144d",
        "name": "Préparer JSON Écriture",
        "func": "let sector = Number(flow.get(\"secteur\") ?? 0);\nlet keyA = (flow.get(\"cle\") ?? \"FFFFFFFFFFFF\").toString().trim().toUpperCase();\nlet text_data = (flow.get(\"data_input_text\") ?? \"\").toString();\nlet number_data = parseFloat(flow.get(\"data_input_number\") ?? 0);\n\nif (!/^[0-9A-F]{12}$/.test(keyA)) keyA = \"FFFFFFFFFFFF\";\nif (isNaN(sector)) sector = 0;\nsector = Math.max(0, Math.min(15, sector));\nif (isNaN(number_data)) number_data = 0;\n\nmsg.payload = {\n    action: \"write_sector\",\n    sector: sector,\n    keyA: keyA,\n    text: text_data.substring(0, 32),\n    number: number_data\n};\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 430,
        "y": 240,
        "wires": [
            [
                "json-converter"
            ]
        ]
    },
